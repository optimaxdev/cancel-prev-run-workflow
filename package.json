{
  "name": "cancel-prev-run-workflow",
  "version": "0.0.1",
  "private": true,
  "description": "This is a GitHub Action that will cancel any previous runs that are not completed for a given workflow.",
  "main": "dist/index.js",
  "repository": "git@github.com:optimaxdev/cancel-prev-run-workflow.git",
  "author": "optimaxdev <optimaxdev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "build": "yarn ncc build src/index.ts"
  },
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.1",
    "@vercel/ncc": "^0.25.1",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.2.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.5"
  }
}
